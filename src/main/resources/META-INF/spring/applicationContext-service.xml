<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<import resource="applicationContext.xml"/>
	
	<bean id="accountService" class="br.com.bufunfa.finance.account.service.AccountSystemServiceImpl" />
	<bean id="transactionService" class="br.com.bufunfa.finance.account.service.TransactionServiceImpl" />
	
	<!-- Beans Helper dos Testes -->
	<bean id="accountSystemServiceHelper" class="br.com.bufunfa.finance.account.service.util.AccountSystemHelper" />
	<bean id="accountHelper" class="br.com.bufunfa.finance.account.service.util.AccountHelper" />

</beans>
